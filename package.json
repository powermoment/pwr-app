{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run dev:css\" \"netlify dev\"",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "start": "cross-env NODE_ENV=production netlify dev",
    "lint": "eslint --ignore-path .gitignore --ext .js,.jsx,.ts,.tsx ./app/**.** --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "@headlessui/react": "^1.6.4",
    "@heroicons/react": "^1.0.6",
    "@netlify/functions": "^1.0.0",
    "@remix-run/netlify": "^1.6.0",
    "@remix-run/node": "^1.5.1",
    "@remix-run/react": "^1.5.1",
    "@remix-run/serve": "^1.5.1",
    "@supabase/supabase-js": "^1.35.3",
    "@tailwindcss/forms": "^0.5.2",
    "cross-env": "^7.0.3",
    "husky": "^8.0.1",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hot-toast": "^2.2.0",
    "remix-auth": "^3.2.2",
    "remix-auth-form": "^1.1.2",
    "remix-auth-github": "^1.1.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.5.1",
    "@remix-run/eslint-config": "^1.6.0",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.17",
    "autoprefixer": "^10.4.7",
    "concurrently": "^7.2.1",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-tailwindcss": "^3.5.2",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.1.2",
    "typescript": "^4.6.4"
  },
  "engines": {
    "node": ">=14"
  },
  "husky": {
    "hooks": {
      "pre-commit": "CI=1 lint-staged"
    }
  },
  "lint-staged": {
    "*.+(js|mjs|ts|tsx|css|md|yml)": [
      "prettier --write"
    ]
  }
}
